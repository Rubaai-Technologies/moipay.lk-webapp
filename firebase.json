{
  "hosting": {
    "public": ".",
    "ignore": [
      "firebase.json",
      "**/.*",
      "**/node_modules/**"
    ],
    "rewrites": [
      {
        "source": "/privacy-policy",
        "site": "moipay-policies"
      },
      {
        "source": "/privacy-policy/**",
        "site": "moipay-policies"
      },
      {
        "source": "**",
        "destination": "/index.html"
      }
    ]
  }
}